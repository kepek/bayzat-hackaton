{{#app-navbar}}
  History
{{/app-navbar}}

{{#app-page class="navbar-through toolbar-through" data-page="history"}}
  <div class="page-content">
    <div class="content-block-title">2016</div>
    <div class="list-block">
      <ul>
        {{#each history as |user|}}
          <li class="item-content">
            <div class="item-media"><i class="icon icon-form-calendar"></i></div>
            <div class="item-inner">
              <div class="item-title">{{user.date}}</div>
              <div class="item-after"><span class="badge">{{user.steps}}</span></div>
            </div>
          </li>
        {{/each}}
      </ul>
      <div class="list-block-label">List block label text goes here</div>
    </div>
  </div>
  {{outlet}}
{{/app-page}}
